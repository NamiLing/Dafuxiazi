<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="bupt.nihongoapp.dafuxiazi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".KanaGridActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".KanaStudyActivity"
            android:parentActivityName=".KanaGridActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bupt.nihongoapp.dafuxiazi.KanaGridActivity" />

        </activity>
    </application>

</manifest>